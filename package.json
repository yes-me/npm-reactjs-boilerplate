{
  "name": "npm-reactjs-boilerplate",
  "version": "1.0.2",
  "description": "npm reactjs boilerplate",
  "main": "build/index.js",
  "files": [
    "src/",
    "README.md"
  ],
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:fn": "APP_ID=FN jest --detectOpenHandles --forceExit",
    "test:pbcfs": "APP_ID=PBCFS jest --detectOpenHandles --forceExit",
    "test:coverage": "jest --coverage",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:ci": "npm run lint:js && npm run lint:css && npm run prettier",
    "lint:js": "eslint 'src/**/*{.js,.jsx}'",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:fix": "eslint --fix 'src/**/*{.js,.jsx,.json}' && stylelint --fix 'src/**/*.css'",
    "clean": "rimraf dist/**",
    "clean-lint": "npm run clean && npm run lint",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --no-colors",
    "prettier": "prettier -l 'src/**/*{.js,.jsx,.json,.css}'",
    "prettier:fix": "npm run prettier -- --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yes-me/npm-reactjs-boilerplate.git"
  },
  "author": "Amy Huang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yes-me/npm-reactjs-boilerplate/issues"
  },
  "homepage": "https://github.com/yes-me/npm-reactjs-boilerplate#readme",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "axios": "^0.18.0",
    "core-js": "^2.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs2": "^7.1.2",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-no-constructor-bind": "^1.2.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "jest-css-modules": "^1.1.0",
    "jsonlint-cli": "^1.0.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "redux-mock-store": "^1.5.1",
    "regenerator-runtime": "^0.10.5",
    "rimraf": "^2.6.2",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.4"
  }
}
